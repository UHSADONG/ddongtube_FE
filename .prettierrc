{
  "semi": true,
  "singleQuote": true,
  "printWidth": 100,
  "tabWidth": 2,
  "trailingComma": "all",
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": ["^react$", "^react-router-dom$", "<THIRD_PARTY_MODULES>", "^[a-zA-Z]", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
