    "event-source-polyfill": "^1.0.31",
    "heic2any": "^0.0.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-player": "^2.16.0",
    "react-router": "^7.5.2",
    "zustand": "^5.0.3"
