    "eslint-plugin-prettier": "^5.2.5",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.4",
    "prettier": "^3.5.3",
    "puppeteer": "^24.5.0",
    "tailwindcss": "^4.0.16",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.28.0",
    "vite": "^6.2.0",
    "vite-plugin-pwa": "^1.0.0",
